#!/bin/bash
cd "$(dirname "$0")/../"

source ./bin/begin.sh

python3 -c '
import asyncio
import sys
from backend.search_FAISS import full_pipeline, init_resources

question_default="Валера"

async def main():
    init_resources()
    if len(sys.argv) > 1:
        q=sys.argv[1]
    else:
        q=question_default
    res=await full_pipeline(q)
    print(res)

asyncio.run(main())
'