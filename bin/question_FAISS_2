#!/bin/bash
cd "$(dirname "$0")/../"

source ./bin/begin.sh

python3 -c '
import asyncio
import sys
from backend.question_analyzer_FAISS_2 import full_pipeline, init_resources

question_default="Валера"

async def main():
    backend.create_FAISS_2.init_resources()
    #backend.create_FAISS_2.build_or_update_index()
    q=sys.argv[1]
    if str(q) == "" :
	q=question_default
    res=full_pipeline(q)
    print(res)

asyncio.run(main())
'